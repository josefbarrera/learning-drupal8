{#
/**
 * @file
 * Learning's theme implementation to display a single page.
 */
#}
<div class="page">
  {% if page.header %}
    <header>
      {{ page.header }}
    </header>
  {% endif %}

  <main>
    {{ page.content }}
  </main>

  {% if page.footer %}
    <footer>
      {{ page.footer }}
    </footer>
  {% endif %}
</div>
